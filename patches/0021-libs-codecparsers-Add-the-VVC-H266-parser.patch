+/*****  Utils ****/
+static inline gint
+floor_log2 (guint32 x)
+{
+  int result = 0;
+
+  if (x == 0) {
+    return -1;
+  }
+
+  if (x & 0xffff0000) {
+    x >>= 16;
+    result += 16;
+  }
+
+  if (x & 0xff00) {
+    x >>= 8;
+    result += 8;
+  }
+
+  if (x & 0xf0) {
+    x >>= 4;
+    result += 4;
+  }
+
+  if (x & 0xc) {
+    x >>= 2;
+    result += 2;
+  }
+
+  if (x & 0x2) {
+    x >>= 1;
+    result += 1;
+  }
+
+  return result;
+}
